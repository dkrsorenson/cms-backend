{
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["@typescript-eslint"],
    "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
    "env": {
        "node": true,
        "es2021": true
    },
    "overrides": [
    ],
    "rules": {
        "prefer-const": "warn",
        "import/no-unresolved": "off",
        "@typescript-eslint/no-unused-vars": "warn"
    }
}
